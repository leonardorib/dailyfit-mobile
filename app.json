{
  "expo": {
    "name": "Dailyfit",
    "slug": "dailyfit",
	"version": "1.0.0",
    "orientation": "portrait",
	"userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
	  "bundleIdentifier": "com.leonardo.dailyfit",
	  "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
	  "package": "com.leonardo.dailyfit",
	  "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
